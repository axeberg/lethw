    "url": "git+https://github.com/axelolsson/lethw.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/axelolsson/lethw/issues"
  },
  "homepage": "https://github.com/axelolsson/lethw#readme",
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
