    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/prompt": "^8.0.0",
    "@commitlint/prompt-cli": "^8.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.4.0"
  },
  "scripts": {
    "commit": "git-cz",
    "test": "echo \"Error: no test specified\" && exit 1"
